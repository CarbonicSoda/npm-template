{
	"name": "@wavim/&pack",
	"version": "0.0.0-res",
	"description": "&pack",
	"lint-staged": {
		"*.ts": [
			"eslint --fix --no-warn-ignored --max-warnings=0",
			"prettier --write"
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/wavim/&pack"
	},
	"license": "MIT",
	"keywords": [],
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"require": "./dist/index.cjs",
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"scripts": {
		"prepublish": "pnpm run build",
		"prepare": "husky",
		"build": "pkgroll --clean-dist --minify",
		"watch": "pkgroll --watch"
	},
	"devDependencies": {
		"@eslint/js": "^9.33.0",
		"@stylistic/eslint-plugin": "^5.2.3",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.1.8",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.5",
		"pkgroll": "^2.15.3",
		"prettier": "^3.6.2",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.39.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
